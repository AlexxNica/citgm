{
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "single"
    ],
    "no-trailing-spaces": 2,
    "comma-spacing": [
      "error",
      { "before": false, "after": true }
    ],
    "eqeqeq": [
      "error",
      "always"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "max-len": [2, 80],
    "semi": [
      2,
      "always"
    ]
  },
  "env": {
    "node": true
  },
  "extends": "eslint:recommended"
}
